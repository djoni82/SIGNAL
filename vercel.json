{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {},
  "outputDirectory": ".",
  "buildCommand": "echo 'No build required for static site'",
  "installCommand": "echo 'No dependencies to install'"
} 